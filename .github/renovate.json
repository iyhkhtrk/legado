{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard"
  ],
  "gradle": {
    "managerFilePatterns": [
      "/(^|/)build.gradle$/",
      "/(^|/)settings.gradle$/"
    ]
  },
  "github-actions": {
    "managerFilePatterns": [
      "/.github/workflows/.*.yml$/"
    ]
  }
}